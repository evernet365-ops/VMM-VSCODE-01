# VIVOTEK smoke-only profile example
# Copy to .env.local (or set with PowerShell $env:*)

FEATURE_VMS_VIVOTEK_CGI=true

# Connector endpoint
SMOKE_CONNECTOR_URL=http://localhost:3013
SMOKE_TIMEOUT_MS=5000
ALLOW_INSECURE_TLS=false

# VIVOTEK targets
VIVOTEK_NVR_BASE_URL=https://<nvr-ip>:3443
VIVOTEK_IPCAM_BASE_URL=http://<camera-ip>:80
VIVOTEK_CAMERA_ID=C_1

# Auth
VIVOTEK_USERNAME=admin
VIVOTEK_PASSWORD=<password>

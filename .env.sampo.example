# SAMPO smoke-only profile example
# Copy to .env.local (or set with PowerShell $env:*)

FEATURE_VMS_SAMPO_CGI=true

# Connector endpoint
SMOKE_CONNECTOR_URL=http://localhost:3013
SMOKE_TIMEOUT_MS=5000
ALLOW_INSECURE_TLS=false

# SAMPO targets
SAMPO_NVR_BASE_URL=https://<nvr-ip>:443
SAMPO_CAMERA_BASE_URL=http://<camera-ip>:80
SAMPO_PRIVATE_HOST=<nvr-ip>
SAMPO_PRIVATE_PORT=30000

# Auth
SAMPO_USERNAME=admin
SAMPO_PASSWORD=<password>
